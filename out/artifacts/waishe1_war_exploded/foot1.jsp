<!-- Mini Cart Start -->
<aside class="mini-cart" id="miniCart">
    <div class="mini-cart-wrapper">
        <div class="mini-cart__close">
            <a href="#" class="btn-close"><i class="flaticon-cross"></i></a>
        </div>
        <div class="mini-cart-inner">
            <h3 class="mini-cart__heading mb--45">购物车</h3>
            <div class="mini-cart__content">
                <ul class="mini-cart__list">
                    <!-- 购物车-->
                    <c:forEach items="${cart.cartItems}" var="cartSub">
                        <li class="mini-cart__product">
                            <a href="#" class="mini-cart__product-remove">
                                <i class="flaticon-cross"></i>
                            </a>
                            <div class="mini-cart__product-image">
                                <img src="${pageContext.request.contextPath}/${cartSub.value.product.img_t1}" alt="${cartSub.value.product.pname}">
                            </div>
                            <div class="mini-cart__product-content">
                                <a class="mini-cart__product-title" href="product-details.html">${cartSub.value.product.pname}</a>
                                <span class="mini-cart__product-quantity">${cartSub.value.buyNum} x $${cartSub.value.product.new_price}</span>
                            </div>
                        </li>
                    </c:forEach>

                </ul>
                <div class="mini-cart__total">
                    <span>Subtotal</span>
                    <span class="ammount">$${cart.total}</span>
                </div>
                <div class="mini-cart__buttons">
                    <a href="${pageContext.request.contextPath}/cart.jsp" class="btn btn-fullwidth btn-bg-primary mb--20">View Cart</a>
                    <a href="checkout.html" class="btn btn-fullwidth btn-bg-primary">Checkout</a>
                </div>
            </div>
        </div>
    </div>
</aside>
